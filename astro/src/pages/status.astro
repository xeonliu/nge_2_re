---

import Table from "../components/react/Table";
import Layout from "../layouts/Layout.astro";
import data from "../data/files.json";
import MyProject from "../ts/parse_data";
import MultiTable from "../components/react/MultiTable";
import LeaderboardTable from "../components/react/LeaderboardTable.jsx";

const proj = new MyProject();

for (const file of data) {
  proj.addRawFile(file);
}

const result = proj.root.getContent();
---

<Layout title="Status">
  <div class="l-docs">
    <div class="l-docs__main">
      <div class="p-section">
        <main class="u-fixed-width">
          <MultiTable data={result} client:load />
          <LeaderboardTable client:load />
        </main>
      </div>
    </div>
  </div>
</Layout>
